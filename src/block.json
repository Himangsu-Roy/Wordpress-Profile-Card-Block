{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "b-blocks/profile-card",
  "version": "1.0.0",
  "title": "Profile Card",
  "category": "widgets",
  "description": "Short description of the Test Purpose",
  "keywords": ["profile card", "proifle", "card"],
  "textdomain": "b-blocks",
  "attributes": {
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "purposeType": {
      "type": "string",
      "default": "test"
    },
    "colors": {
      "type": "object",
      "default": {
        "color": "black",
        "bg": "#B1C5A4"
      }
    },
    "profileImage": {
      "type": "object",
      "default": {
        "url": "",
        "alt": "Profile Image"
      }
    },
    "name": {
      "type": "string",
      "default": "Sarah Anderson"
    },
    "title": {
      "type": "string",
      "default": "Senior Product Designer"
    },
    "stats": {
      "type": "object",
      "default": {
        "projects": "1.2k",
        "followers": "8.5k",
        "following": "4.7k"
      }
    },
    "bio": {
      "type": "string",
      "default": "Creative designer with 5+ years of experience in digital product design and brand identity."
    },
    "skills": {
      "type": "array",
      "default": ["UI/UX", "Branding", "Motion"]
    }
  },
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "example": {
    "attributes": {}
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./view.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}
